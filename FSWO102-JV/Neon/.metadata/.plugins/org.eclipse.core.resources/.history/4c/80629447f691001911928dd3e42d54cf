package src.agnes.ejb;

public class AddBean {

	private int i, j, result;

	// Constructors.
	public AddBean() {
		this(0, 0);
	}	
	public AddBean(int i, int j) {
		this.i = i;
		this.j = j;
		result = 0;
	}
	
	// Business logic.
	public void add() {
		result = i + j;
	}
	public void add(int i, int j) {
		
	}
	// Accessors, use as needed.
	public int getI() {
		return i;
	}

	public void setI(int i) {
		this.i = i;
	}

	public int getJ() {
		return j;
	}

	public void setJ(int j) {
		this.j = j;
	}

	public int getResult() {
		return result;
	}
	/*
	public void setResult(int result) {
		this.result = result;
	}
	*/
	
}
